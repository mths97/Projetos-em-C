#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
    int a,b,c;
    float x1,x2,delta;

    setlocale (LC_ALL, "portuguese");
    printf ("Digite valor a: ");
    scanf ("%d", &a);
    printf ("Digite valor b: ");
    scanf ("%d", &b);
    printf ("Digite valor c: ");
    scanf ("%d", &c);

    delta = (b*b)-(4*a*c);

    x1 = (-b+sqrt(delta))/(2*a);
    x2 = (-b-sqrt(delta))/(2*a);

    if (delta < 0)
    {
        printf ("A equação não tem raízes reais");
    } else
    {
        printf ("Valor de x1: ", x1);
        printf ("Valor de x1: ", x2);
    }


    return 0;
}
